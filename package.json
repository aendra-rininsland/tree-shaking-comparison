{
  "name": "tree-shaking-comparison",
  "version": "1.0.0",
  "description": "Comparison of various tree-shakers",
  "main": "index.js",
  "scripts": {
    "build-webpack": "webpack --profile --display-modules --display-reasons --optimize-minimize",
    "build-rollup": "rollup -c -o rollup-bundle.js",
    "uglify": "uglifyjs rollup-bundle.js -c -m -o rollup-bundle.min.js",
    "build-jspm": "jspm bundle index.js jspm-bundle.js --minify",
    "build": "npm run build-webpack && npm run build-rollup && npm run uglify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "d3": "^4.2.6",
    "d3-scale": "^1.0.3",
    "jspm": "^0.16.48",
    "rollup": "^0.37.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.7.5",
    "webpack": "^2.1.0-beta.27"
  },
  "jspm": {
    "dependencies": {
      "d3": "npm:d3@^4.2.6"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
